<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
    <Id>Teacher M</Id>
    <Version>1.2</Version>
    <ProviderName>敬爱的梅老师</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>
    <DisplayName>GPT to Word Plugin</DisplayName>
    <Description>基于 Markdown 转换的 Word 插件</Description>
    <Hosts>
        <Host Name="Word"/>
    </Hosts>
    <DefaultSettings>
        <SourceLocation DefaultValue="https://your-server-or-local-path/index.html"/>
    </DefaultSettings>
    <Permissions>ReadWriteDocument</Permissions>
</OfficeApp>
